{
  "manifest_version": 2,
  "name": "Media Speed Controller",
  "version": "0.0.1",
  "description": "HTML5 video and audio speed controller",
  "homepage_url": "https://github.com/pd4d10/media-speed-controller",
  "background": {
    "scripts": ["background.js"]
  },
  "permissions": ["<all_urls>"],
  "browser_action": {
    "default_title": "Media Speed Controller",
    "default_popup": "popup.html"
  }
}
